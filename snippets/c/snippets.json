{
  "Printf": {
    "prefix": "pr",
    "body": ["printf(\"${1:%s}\\n\"${1/[^%]*/%s/g}${1/[^%]*/%s/g:+, }$2);$0"],
    "description": "Printf with newline"
  },
  "Main Function": {
    "prefix": "main",
    "body": ["int main(int argc, char *argv[]) {", "\t$1", "\treturn 0;", "}"],
    "description": "Main function custom"
  },
  "For Loop": {
    "prefix": "for",
    "body": [
      "for (int ${1:i} = ${2:0}; ${1:i} < ${3:count}; ${1:i}${4:++}) {",
      "\t$5",
      "}"
    ],
    "description": "For loop"
  },
  "If Statement": {
    "prefix": "if",
    "body": ["if ($1) {", "\t$2", "}"],
    "description": "If statement"
  }
}
